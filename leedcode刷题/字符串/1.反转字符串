把字符串存入字符数组 进行数组反转
class Solution {
public:
    
    void reverse(vector<char>& s)
    {
        int n=s.size()-1;
        int start=0;
        while(start<n)
        {  char tmp;
            tmp=s[start];
            s[start]=s[n];
            s[n]=tmp;
         start++;
         n--;
        }
        return;
    }
    void reverseString(vector<char>& s) {
        reverse(s);
        return;
        
        
    }
};
